#cb-starter{border: dashed 2px #ddd; line-height: 80px; padding: 0 20px; border-radius: 10px; color:#666; position: absolute; top: 10px; left: 10px; right: 10px; font-weight: bold; text-align: right;}
/*#cb-source, #cb-content, aside{list-style-type: none; margin: 0; padding: 0;-webkit-touch-callout: none;-webkit-user-select: none;-khtml-user-select: none;-moz-user-select: none;-ms-user-select: none; user-select: none;}
