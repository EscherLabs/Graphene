#cb-container {overflow: hidden}
#cb-content .cb-placeholder, #cb-content > li { margin: 0; padding: 0; position: relative}
#cb-source .cb-placeholder {display: none !important; background:#fff;}
#cb-content .cb-placeholder{display: block; float: left;
	-moz-box-shadow: inset 0 0 1px #ddd;
	-webkit-box-shadow: inset 0 0 1px #666;
	box-shadow: inner 0 0 1px #666;
}
#cb-content > li {float: left; display: block;}
#cb-content > li.pull-right {float: right;}
#cb-content > li{width: 100%}
#cb-content > li.cb-placeholder.source{width: 100%;height: 40px !important}

#cb-content > li.selected > .actions, #cb-content > li:hover > .actions{display: inline-block}
#cb-content > li > .actions{display: none; z-index: 10; position: absolute; top:0;right:0;}

#cb-content > li:before{content:'';z-index: 10; position: absolute; top: 0; bottom: 0; left: 0; right: 0;}
#cb-content > li{outline: dashed 1px transparent;}
#cb-content > li:hover{outline-color:#ccc;z-index: 1;}
#cb-content > li.selected{outline-color:#333;z-index: 2;}
#cb-content > li.selected.noveil:before{display:none;}
#cb-content > li.selected{
	background: #FFFFF8;
}

.noedit #cb-content > li:hover:before{border:none;}

#cb-content{min-height: 200px; width: 100%;position: relative;}
#cb-content: after{
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}
.cobler-li-content{outline:none;}
#cb-content .cb-placeholder{border: dashed #ccc; border-width: 0; background:#fff; opacity:.7}

