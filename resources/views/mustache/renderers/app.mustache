    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body class="
  {{#topbar_enabled}}
  topbar
  {{/topbar_enabled}}
  {{#sidemenu_enabled}}
  sidemenu
  {{/sidemenu_enabled}}
">
    {{#topbar_enabled}}
    <nav class="navbar navbar-inverse navbar-fixed-top">
      <div class="container-fluid">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/">
            {{#site.theme.icon}}
            <i class="fa fa-{{site.theme.icon}} fa-fw" style="font-size: 36px;margin: -8px 0px 0px -8px;float: left;"></i>
            {{/site.theme.icon}}
            &nbsp;{{site.name}}
          </a>
          <ul class="nav navbar-nav navbar-right hidden-xs">
            <li><a href="#"><h4 style="margin:0">{{ $name}}</h4></a></li>
          </ul>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
            <li class="dropdown">
              {{#logged_in}}
              <a href="#" class="dropdown-toggle user-info" data-toggle="dropdown" role="button">
                <img class="gravatar" src="https://www.gravatar.com/avatar/{{user_md5_email}})?d=mm" /> 
                {{user.first_name}} {{user.last_name}} 
                <span class="caret"></span>
              </a>
              <ul class="dropdown-menu">
                <li><a href="{{ url.root }}"><i class="fa fa-dashboard"></i> Dashboard</a></li>
                {{#is_admin}}
                  <li><a href="{{ url.admin }}"><i class="fa fa-gear"></i> Admin</a></li>
                {{/is_admin}}  
                <li><a href="{{ url.logout }}"><i class="fa fa-times-circle"></i> Logout</a></li>
              </ul>
              {{/logged_in}}
              {{^logged_in}}
              <a href="#" class="dropdown-toggle user-info" data-toggle="dropdown" role="button">
                <img class="gravatar" src="https://www.gravatar.com/avatar/?d=mm&f=y" /> Guest 
                <span class="caret"></span>
              </a>
              <ul class="dropdown-menu">
                <li><a href="{{ url.login }}"><i class="fa fa-sign-in fa-fw"></i> Login</a></li>
              </ul>
              {{/logged_in}}
            </li>
          </ul>
          <ul class="nav navbar-nav navbar-right visible-xs-block">
            <!--<li><a href="/"><i class="fa fa-dashboard"></i> Dashboard</a></li>-->
            {{#apps}}
              <li><a href="/app/{{slug}}"><i class="fa fa-{{#icon}}{{icon}}{{/icon}}{{^icon}}cube{{/icon}} fa-fw"></i>&nbsp; {{name}}</a></li>
            {{/apps}}
          </ul>
          <!--form class="navbar-form navbar-right">
            <input type="text" class="form-control" placeholder="Search...">
          </form-->
          {{#app_mode}}{{> app_titlebar}}{{/app_mode}}
          {{^app_mode}}{{> dashboard_titlebar}}{{/app_mode}}
        </div>
      </div>
    </nav>
    {{/topbar_enabled}}
    {{#sidemenu_enabled}}
    <div class="col-sm-3 col-md-2 sidebar">
      <ul class="nav nav-sidebar">
        {{#links}}
          <li><a data-toggle="collapse" href="#collapse{{ id }}">{{ name }}</a></li>
          <ul class="collapse"  id="collapse{{ id }}">
          {{#pages}}
              <li><a href="/page/{{group_slug}}/{{ slug }}{{^topbar_enabled}}?topbar=false{{/topbar_enabled}}"><i class="fa fa-{{#icon}}{{icon}}{{/icon}}{{^icon}}file{{/icon}} fa-fw"></i>&nbsp; {{ name }}</a></li>
          {{/pages}}
          {{#app_instances}}
              <li><a href="/app/{{group_slug}}/{{ slug }}{{^topbar_enabled}}?topbar=false{{/topbar_enabled}}"><i class="fa fa-{{#icon}}{{icon}}{{/icon}}{{^icon}}cube{{/icon}} fa-fw"></i>&nbsp; {{ name }}</a></li>
          {{/app_instances}}
          </ul>
        {{/links}}
      </ul>
    </div>
    {{/sidemenu_enabled}}
    <div class="container-fluid" id="main-container">
      <div class="row">
        <div class="col-sm-12 main">


              <div class="row " id="page_layout">

              </div>

        </div>
      </div>

    </div>
